{
  "name": "tornevall/tornelib-php-netcurl",
  "description": "Yet another Curl Library to handling web calls",
  "minimum-stability": "stable",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.3",
    "ext-soap": "*",
    "ext-curl": "*"
  },
  "authors": [
    {
      "name": "Tomas Tornevall",
      "email": "thorne@tornevall.net",
      "homepage": "https://tornevall.net",
      "role": "Developer"
    }
  ],
  "autoload": {
    "files": [
      "source/Modules/netcurl_init.php",
      "source/Modules/drivers.php",
      "source/Modules/ssl.php",
      "source/Addons/netbits.php",
      "source/Exception/Exception.php",
      "source/Model/Type/authtypes.php",
      "source/Model/Type/curlobject.php",
      "source/Model/Type/datatypes.php",
      "source/Model/Type/drivers.php",
      "source/Model/Type/environment.php",
      "source/Model/Type/ip_protocols.php",
      "source/Model/Type/postmethods.php",
      "source/Model/Type/resolvers.php",
      "source/Model/Type/responses.php",
      "source/Modules/network.php",
      "source/Modules/curl.php",
      "source/Modules/soap.php"
    ],
    "psr-4": {
      "TorneLIB\\": "source"
    }
  }
}
